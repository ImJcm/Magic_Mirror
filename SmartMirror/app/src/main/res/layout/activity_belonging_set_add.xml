<?xml version="1.0" encoding="utf-8"?>
<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:orientation="vertical"
    android:id="@+id/belongingSetAddLayout"
    tools:context=".activities.BelongingSetAddActivity">
    <include layout="@layout/custom_appbar"></include>
    <androidx.recyclerview.widget.RecyclerView
        android:layout_width="match_parent"
        android:layout_height="0dp"
        android:layout_weight="8"
        android:layout_marginVertical="10dp"
        android:layout_marginHorizontal="10dp"
        android:layout_gravity="center_horizontal"
        android:id="@+id/belongingItemRecyclerView"></androidx.recyclerview.widget.RecyclerView>


</LinearLayout>